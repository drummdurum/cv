{
  "name": "cv-sebastian-drumm",
  "version": "1.0.0",
  "description": "Sebastian Drumm CV website med EJS og Tailwind CSS",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "build": "tailwindcss -i ./public/css/input.css -o ./public/css/output.css --minify",
    "build:css": "tailwindcss -i ./public/css/input.css -o ./public/css/output.css --watch",
    "railway:build": "npm install && npm run build"
  },
  "keywords": ["cv", "portfolio", "ejs", "tailwind"],
  "author": "Sebastian Drumm",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
